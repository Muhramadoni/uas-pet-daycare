{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "TURSO_URL": "@turso_url",
    "TURSO_AUTH_TOKEN": "@turso_auth_token",
    "JWT_SECRET": "@jwt_secret",
    "PORT": "5000"
  }
}
